<h3>Account Information</h3>
<div class="container-fluid">
<div class="span5 offset3">
<% provide(:title, "Personal info") %>

<p>
  <b>First name:</b>
  <%= @user.first_name %>
</p>

<p>
  <b>Last name:</b>
  <%= @user.last_name %>
</p>

<p>
  <b>E mail:</b>
  <%= @user.email %>
</p>

<p>
  <b>Username:</b>
  <%= @user.username %>
</p>

<p>
  <b>Token:</b>
  <%= @user.token %>
</p>


<%= link_to 'Edit', edit_user_path(@user), class: "btn" %>

</div>
</div>
